    line-height:140%;
    
    color:{color:text};
    {block:iftextglow}
    text-shadow: 0 0 2px {color:text glow}, 0 0 2px {color:text glow};
    {/block:iftextglow}
    {block:iftextoutline}
        
        text-shadow: -1px 0 {color:text outline}, 0 1px {color:text outline}, 1px 0 {color:text outline}, 0 -1px {color:text outline}{block:iftextglow} , 0 0 2px {color:text glow}, 0 0 2px {color:text glow}{/block:iftextglow};
        
    {/block:iftextoutline}
}

