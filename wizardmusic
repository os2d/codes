<!--Music Player Start-->
{block:ifmusicplayer}
<script>
	        let songs = [
	            {"title": "{text:song 1 title}", "url": "{text:song 1 url}"},
	            {"title": "{text:song 2 title}", "url": "{text:song 2 url}"},
	            {"title": "{text:song 3 title}", "url": "{text:song 3 url}"},
	            {"title": "{text:song 4 title}", "url": "{text:song 4 url}"},
	            {"title": "{text:song 5 title}", "url": "{text:song 5 url}"}
	            ]
	        
	        const $songtitle = document.querySelector(".song-title");
	        const $prevsong = document.querySelector(".prev-song");
	        const $nextsong = document.querySelector(".next-song");
	        const $playpause = document.querySelector(".play-pause");
	        const $musicer = document.querySelector(".musicer");
	        
	        var songNum = 0;
	        var isSongPlaying = false;
	        
	        function changeSong(targetSongNum){
	            if(targetSongNum < 0) songNum = 4;
	            if(targetSongNum > 4) songNum = 0;
	            
	            $songtitle.innerHTML = `<marquee>${songs[songNum].title}</marquee>`;
	            $musicer.src = songs[songNum].url;
	            playPause();
	            playPause();
	        }
	        
	        function playPause(){
	            isSongPlaying = !isSongPlaying;
	            if(isSongPlaying){
	                $musicer.pause();
	                $playpause.innerHTML = "<span class='material-symbols-outlined'>play_circle</span>"
	            } else{
	                $musicer.play();
	                $playpause.innerHTML = "<span class='material-symbols-outlined'>stop_circle</span>"
	            }
	            
	        }
	        
	        function showSongTitle(){
	            $songtitle.classList.add("show-song-title");
	        }
	        function hideSongTitle(){
	            $songtitle.classList.remove("show-song-title");
	        }
	        
	        changeSong(0);
	        playPause();
	        
	        $prevsong.addEventListener("click", function(){
	            songNum--
	            changeSong(songNum);
	        })
	        $nextsong.addEventListener("click", function(){
	            songNum++
	            changeSong(songNum);
	        })
	        $playpause.addEventListener("click", function(){
	            playPause();
	        })
	    </script>
	    {/block:ifmusicplayer}
<!--Music Player End-->


