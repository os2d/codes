a{
    
    color:{color:link};
    text-decoration:none;
    
    {block:iflinkglow}
    text-shadow:0 0 2px {color:link glow}, 0 0 2px {color:link glow};
    {/block:iflinkglow}
     
    {block:iflinkoutline}
    
    text-shadow: -1px 0  {color:link outline}, 0 1px  {color:link outline}, 1px 0  {color:link outline}, 0 -1px  {color:link outline}{block:iflinkglow}, 0 0 3px {color:link glow}, 0 0 3px {color:link glow} {/block:iflinkglow};
    
    {/block:iflinkoutline}
    
    {block:iflinkbold}
    font-weight:bold;
    letter-spacing:1px;
    {/block:iflinkbold}
    
    {block:iflinkgradiatedsecondcolor}
text-shadow:none;
background: -webkit-linear-gradient(left,{color:link},{color:link gradiated second color});
    -webkit-background-clip: text;
    -webkit-text-fill-color: transparent;

{block:iflinkglow}

filter:drop-shadow(0 0 1px {color:link glow});
 
{/block:iflinkglow}

{block:iflinkoutline}

filter: drop-shadow(-1px 0px {color:link outline}) drop-shadow(1px 0px {color:link outline}) drop-shadow(0px 1px {color:link outline}) drop-shadow(0px -1px {color:link outline}) {block:iflinkglow} drop-shadow(0 0 1px{color:link glow}) {/block:iflinkglow};
 
{/block:iflinkoutline}

{/block:iflinkgradiatedsecondcolor}

{block:iflinkrainbowgradient}
text-shadow:none;
background:-webkit-linear-gradient(left,#f61538,#fe681e,#fffc24,#0edf36,#106aff,#9c14ff, #f736c4);
    -webkit-background-clip: text;
    -webkit-text-fill-color: transparent;
{/block:iflinkrainbowgradient}

{block:ifnotlinkgradiatedsecondcolor}
{block:iflinkhighlight}
background:linear-gradient(0deg, {color:link highlight} 0%, {color:link highlight} 40%, rgba(255,255,255,0) 40%);
{/block:iflinkhighlight}
{/block:ifnotlinkgradiatedsecondcolor}
     
    transition:0.3s;
    -o-transition: all 0.3s ease-out;
    -webkit-transition: all 0.3s ease-out;
    -moz-transition: all 0.3s ease-out;
}

a:hover{
    color:{color:link hover};

    {block:iflinkglow}
    text-shadow:0 0 2px {color:link glow hover}, 0 0 2px {color:link glow hover};
    {/block:iflinkglow}
     
     
    {block:iflinkitalic}
    font-style: italic;
    {/block:iflinkitalic}
     
     {block:iflinkstrikethrough}
    text-decoration: line-through;
    {/block:iflinkstrikethrough}
     
    {block:ifrainbowlinks}
    -webkit-animation:rainbow 1s infinite;
    -ms-animation:rainbow 1s infinite;
    -o-animation:rainbow 1s infinite;
    animation:rainbow 1s infinite;
    {/block:ifrainbowlinks}
     
    {block:ifpastelrainbowlinks}
    -webkit-animation:pastelrainbow 1s infinite;
    -ms-animation:pastelrainbow 1s infinite;
    -o-animation:pastelrainbow 1s infinite;
    animation:pastelrainbow 1s infinite;
    {/block:ifpastelrainbowlinks}
    
     
    {block:iflinkoutline}
    text-shadow: -1px 0  {color:link outline hover}, 0 1px  {color:link outline hover}, 1px 0  {color:link outline hover}, 0 -1px  {color:link outline hover}{block:iflinkglow}, 0 0 3px {color:link glow hover}, 0 0 3px {color:link glow hover} {/block:iflinkglow};
    {/block:iflinkoutline}
     
     
         {block:iflinkgradiatedsecondcolor}
text-shadow:none;
background: -webkit-linear-gradient(left,{color:link hover},{color:link gradiated second color hover});
    -webkit-background-clip: text;
    -webkit-text-fill-color: transparent;

{block:iflinkglow}

filter:drop-shadow(0 0 1px {color:link glow hover});
 
{/block:iflinkglow}

{block:iflinkoutline}

filter: drop-shadow(-1px 0px {color:link outline hover}) drop-shadow(1px 0px {color:link outline hover}) drop-shadow(0px 1px {color:link outline hover}) drop-shadow(0px -1px {color:link outline hover}) {block:iflinkglow} drop-shadow(0 0 1px{color:link glow hover}) {/block:iflinkglow};
 
{/block:iflinkoutline}

{/block:iflinkgradiatedsecondcolor}

{block:ifnotlinkgradiatedsecondcolor}
{block:iflinkhighlight}
background:linear-gradient(0deg, {color:link highlight hover} 0%, {color:link highlight hover} 40%, rgba(255,255,255,0) 40%);
{/block:iflinkhighlight}
{/block:ifnotlinkgradiatedsecondcolor}
     
     
     {block:iflinkdroppedshadow}
text-shadow: 0 1px 1px {color:link dropped shadow hover}, 0 1px 1px {color:link dropped shadow hover}, 0 1px 0px {color:link dropped shadow hover}, 0px 1px 0px {color:link dropped shadow hover},  0px 2px 0px {color:link dropped shadow hover}, 0px 3px 0px {color:link dropped shadow hover},0px 4px 0px {color:link dropped shadow hover};
{/block:iflinkdroppedshadow}
     
    cursor:{select:cursor hover effect};
    
    transition:0.3s;
    -o-transition: all 0.3s ease-out;
    -webkit-transition: all 0.3s ease-out;
    -moz-transition: all 0.3s ease-out;
}

